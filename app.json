{
  "expo": {
    "name": "Sachio Mobile Toilets",
    "slug": "Sachio-Mobile-Toilets",
    "scheme": "sachio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo (1).png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": [
        "applinks:sachio-mobile-web.vercel.app"
      ]
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "sachio",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ]
        }
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "package": "sachio.moblie"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo (1).png"
    },
    "plugins": [
      "./plugins/rnfirebase-app.plugin",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo (1).png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a53a89d-2c0e-4173-b0db-a97808fee1e9"
      }
    }
  }
}
